<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>NooberCong.PDFire</Title>
        <Authors>NooberCong</Authors>
        <Description>Open source PDF2Image rendering, Bookmark parsing and PDF watermarking library</Description>
        <PackageLicenseUrl>https://licenses.nuget.org/MIT</PackageLicenseUrl>
        <PackageIconUrl>https://i.ibb.co/WDn39P1/pdf-fire.png</PackageIconUrl>
        <PackageTags>PDF Render Bookmark Watermark Image</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <TargetFramework>net5.0</TargetFramework>
        <LangVersion>10</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="itext7" Version="7.2.0" />
      <PackageReference Include="PDFium.WindowsV2" Version="1.0.2" />
      <PackageReference Include="PDFiumSharpV2" Version="1.0.1" />
      <PackageReference Include="System.Drawing.Common" Version="6.0.0" />
    </ItemGroup>

</Project>
